{
  "name": "acc-component",
  "version": "1.0.0",
  "svelte": "src/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "dev": "rollup -w --config rollup.config.dev.js",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src/**/*",
    "lint-fix": "eslint src/**/* --fix",
    "prettier": "prettier --check \"src/**/*.{svelte,ts,js,json}\"",
    "prettier-fix": "prettier --write \"src/**/*.{svelte,ts,js,json}\"",
    "test": "jest src",
    "test-watch": "npm run test -- --watch",
    "coverage": "npm run test -- --coverage && open coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-svelte3": "^3.2.0",
    "jest": "^26.6.3",
    "open": "^8.2.1",
    "prettier": "^2.3.1",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.52.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.38.3",
    "svelte-check": "2.2.0",
    "svelte-jester": "^1.7.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "setupFilesAfterEnv": [
    "@testing-library/jest-dom/extend-expect"
  ]
}
